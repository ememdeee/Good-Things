// trim Only
<script type="application/ld+json">
{
  "@context": "https://schema.org",
  "@type": "Vehicle",
  "name": "{year} {make} {model} {trim} {style}",
  "description": "{year} {make} {model} {trim}: Specs, Pricing, Rating",
  "model": "{model}",
  "vehicleModelDate": "{year}",
  "vehicleConfiguration": "{trim} {style}",
  "seatingCapacity": "{specifications.seating.number_of_seats}",
  "vehicleTransmission": "{transmission.type}",
  "cargoVolume": {
    "@type": "QuantitativeValue",
    "value": "{specifications.cargo_dimensions.cargo_volume_cu.ft}"
    },
    "manufacturer": {
        "@type": "Organization",
        "name": "{make}"
    },
  "width": "{specifications.exterior_dimensions.overall_width_in}",
  "fuelCapacity": "{fuel.tank_capacity_gal}",
  "height": "{specifications.exterior_dimensions.overall_height_in}",
  "numberOfDoors": "{specifications.doors}",
  "fuelType": "{fuel.type}",
  "speed": "{engine.horsepower_hp}",
  "brand": {
      "@type": "Brand",
      "name": "{make}"
  },
    "wheelbase": [
      {
        "@type": "QuantitativeValue",
        "value": "{specifications.wheels.spare_wheel_size_in}",
        "unitCode": "CM",
        "name": "Spare Wheel Size"
      },
      {
        "@type": "QuantitativeValue",
        "value": "{specifications.tires.spare_tire_size}",
        "unitCode": "CM",
        "name": "Spare Tire Size"
      }
    ],
    "aggregateRating": {
      "@type": "AggregateRating",
      "ratingValue": "3.5",
      "reviewCount": "11"
    }
}
</script>